<odoo>
  <data>

    <record id="taidt_test_tree" model="ir.ui.view">
      <field name="name">fcxrang.siken.data</field>
      <field name="model">fcxrang.siken.data</field>
      <field name="arch" type="xml">
        <tree>
          <field name="bodyno"/>
          <field name="pilc" string="形式"/>
          <field name="ondo"/>
          <field name="situdo"/>
          <field name="kiatu"/>
          <field name="sdata16" string="正压"/>
          <field name="sdata17" string="负压"/>
          <field name="sdata21" string="(s1-s2)PV值"/>
          <field name="sikenday"/>
        </tree>
      </field>
    </record>

    <record id="taidt_test_form" model="ir.ui.view">
      <field name="name">fcxrang.siken.data</field>
      <field name="model">fcxrang.siken.data</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
            <group col="6">
              <field name="bodyno" col="2"/>
              <field name="pilc" string="形式" col="2"/>
              <field name="ondo" col="2"/>
              <field name="situdo" col="2"/>
              <field name="kiatu" col="2"/>
              <field name="sdata16" string="正压" col="2"/>
              <field name="sdata17" string="负压" col="2"/>
              <field name="sdata21" string="(s1-s2)PV值" col="2"/>
              <field name="sikenday" col="2"/>
            </group>
          </sheet>
        </form>
      </field>
    </record>

    <record id="taidt_test_action_window" model="ir.actions.act_window">
      <field name="name">片均压测试</field>
      <field name="res_model">fcxrang.siken.data</field>
      <field name="view_mode">tree,form</field>
    </record>


  </data>
</odoo>